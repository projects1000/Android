<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.game.myapplication01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/a2z"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/a2z"
        android:supportsRtl="true"
        android:theme="@style/Theme.myapplication01">

        <!-- MainActivity declared as launcher with intent filter -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Remove the duplicate MainActivity declaration -->
        <!-- ErrorActivity declaration -->
        <activity
            android:name=".ErrorActivity"
            android:exported="false" />

    </application>

</manifest>
